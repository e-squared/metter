<%= headline_tag do %>
  <%= edit_link @user %>
  <%= delete_link @user unless @user.admin? %>
<% end %>

<div class="well">
  <dl class="dl-horizontal">
    <dt><%=t ".login" %></dt>
    <dd><%= @user.login %></dd>

    <dt><%=t ".first_name" %></dt>
    <dd><%= @user.first_name %></dd>

    <dt><%=t ".last_name" %></dt>
    <dd><%= @user.last_name %></dd>
  </dl>
</div>

